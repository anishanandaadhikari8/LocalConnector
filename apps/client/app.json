{
  "expo": {
    "name": "Circles",
    "slug": "circles",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#6C8CFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.circles.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#6C8CFF"
      },
      "package": "com.circles.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker"
    ],
    "scheme": "circles"
  }
}
