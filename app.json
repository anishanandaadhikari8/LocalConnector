{
  "expo": {
    "name": "neighborconnect",
    "slug": "neighborconnect",
    "version": "1.0.0",
    "extra": {
      "EXPO_PUBLIC_API_URL": "http://localhost:8082/api/v1",
      "EXPO_PUBLIC_FEATURE_FLAGS": "circles,taskboard,orders,promotions,analytics"
    },
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1e3a8a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1e3a8a"
      }
    },
    "web": {}
  }
} 